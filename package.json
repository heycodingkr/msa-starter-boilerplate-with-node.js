{
  "name": "msa_starter_boilerplate_node.js",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "jason ahn easycodingkr@gmail.com",
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "moleculer-repl": "^0.6.2"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^0.5.1",
    "moleculer": "^0.13.12",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.6",
    "moleculer-web": "^0.8.5",
    "nats": "^1.3.2"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heycodingkr/msa-starter-boilerplate-with-node.js"
  },
  "license": "UNLICENSED"
}
